import java.util.ArrayList;
public class Main {
    public static void main(String[] args) {
       arrayer array2 = new arrayer();
        
       array2.display();
        }
        
    }
    
class arrayer{
    ArrayList<String> nameArrayList = new ArrayList<>();
    ArrayList<Integer> numArrayList = new ArrayList<>();
    
        arrayer(){
         nameArrayList.add("Jenny");
         nameArrayList.add("Marco");
         nameArrayList.add("Adonis");
         numArrayList.add(20);
         
    }
    
    void display(){
        System.out.println("The difference of implementation!!");
        //This is only implementing using print system out
        System.out.println(nameArrayList+ "\n" + numArrayList);
        
        //This is using for loop
        for(String s : nameArrayList){
            System.out.println(s);
    }
    
    }
}
